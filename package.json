{
  "name": "rendex-pos",
  "version": "0.1.0",
  "description": "POS offline (Venezuela) - Node/Express/SQLite/Socket.IO skeleton",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "NODE_ENV=development node src/server.js",
    "test": "mocha --exit",
    "test:e2e": "npx playwright test",
    "lint": "eslint \"**/*.js\" --quiet",
    "format": "prettier --write \"**/*.{js,json,md,html}\"",
    "prepare": "husky install",
    "create-admin": "node scripts/createAdmin.js",
    "migrate": "npx knex migrate:latest --knexfile knexfile.js",
    "migrate:rollback": "npx knex migrate:rollback --knexfile knexfile.js",
    "db:backup": "node scripts/dbBackup.js",
    "import:csv": "node scripts/importCsv.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.0.0",
    "bcrypt": "^5.0.1",
    "chart.js": "^4.5.1",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "lint-staged": "^14.0.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.6",
    "uuid": "^13.0.0"
    ,
    "csv-stringify": "^6.0.5"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^9.0.0",
    "mocha": "^10.2.0",
    "playwright": "^1.37.0",
    "@playwright/test": "^1.37.0",
    "undici": "^6.2.5",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3"
  },
  "playwright": {
    "browsers": ["chromium"]
  }
  ,
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{json,md,html}": [
      "prettier --write",
      "git add"
    ],
    "!public/vendor/**": []
  }
}
